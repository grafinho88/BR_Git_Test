<?xml version="1.0" encoding="utf-8"?>
<xs:schema targetNamespace="http://www.br-automation.com/iat2015/styles/engineering/v1" 
	xmlns:xs="http://www.w3.org/2001/XMLSchema" 
	xmlns:sd="http://www.br-automation.com/iat2015/styles/engineering/v1" 
	xmlns:wt="http://www.br-automation.com/iat2015/widgetTypes/v2" 
	elementFormDefault="qualified">
	<xs:complexType name="widgets.brease.RadialGauge">
		<xs:complexContent>
			<xs:extension base="sd:stylesBase">
				<xs:attribute name="borderWidth" use="optional" type="wt:PixelVal"></xs:attribute>
				<xs:attribute name="borderColor" use="optional" type="wt:ColorCollection"></xs:attribute>
				<xs:attribute name="borderStyle" use="optional" type="wt:BorderStyle"></xs:attribute>
				<xs:attribute name="backColor" use="optional" type="wt:Color"></xs:attribute>
				<xs:attribute name="shadow" use="optional" type="wt:Shadow"></xs:attribute>
				<xs:attribute name="margin" use="optional" type="wt:Margin"></xs:attribute>
				<xs:attribute name="backGroundGradient" use="optional" type="wt:Gradient"></xs:attribute>
				<xs:attribute name="backGround" use="optional" type="wt:ImagePath"></xs:attribute>
				<xs:attribute name="backColorScaleArea1" use="optional" type="wt:Color"></xs:attribute>
				<xs:attribute name="backColorScaleArea2" use="optional" type="wt:Color"></xs:attribute>
				<xs:attribute name="backColorScaleArea3" use="optional" type="wt:Color"></xs:attribute>
				<xs:attribute name="backColorScaleArea4" use="optional" type="wt:Color"></xs:attribute>
				<xs:attribute name="backColorScaleArea5" use="optional" type="wt:Color"></xs:attribute>
				<xs:attribute name="pointerBackColor" use="optional" type="wt:Color"></xs:attribute>
				<xs:attribute name="pointerPathColor" use="optional" type="wt:Color"></xs:attribute>
				<xs:attribute name="pointerPathWidth" use="optional" type="wt:PixelVal"></xs:attribute>
				<xs:attribute name="textColor" use="optional" type="wt:Color"></xs:attribute>
				<xs:attribute name="fontSize" use="optional" type="wt:PixelVal"></xs:attribute>
				<xs:attribute name="fontName" use="optional" type="wt:FontName"></xs:attribute>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
</xs:schema>